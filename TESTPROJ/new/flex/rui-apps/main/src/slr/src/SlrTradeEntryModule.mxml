	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:ComboBox id="openEnded" prompt="Select" labelField="label" change="showNoSlr01ReqFlagField();"/>
						</mx:GridItem>
