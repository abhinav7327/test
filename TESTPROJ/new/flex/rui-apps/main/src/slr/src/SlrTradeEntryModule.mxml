							<mx:TextInput id="commission" editable="true" focusOut="commVal.handleNumericField(numberFormatter);"/>
						</mx:GridItem>
                        <mx:GridItem width="20%" borderThickness="0">
	                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.netamount')}"/>
	                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:Image source="@Embed('../../assets/img/slr/icn_calculator.png')" toolTip="{this.parentApplication.xResourceManager.getKeyValue('slr.label.calcnetamt')}" buttonMode="true" click="calculateNetAmount();"/>
							<mx:TextInput id="netAmount" editable="false"/>
						</mx:GridItem>
