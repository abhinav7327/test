<?xml version="1.0" encoding="utf-8"?>
<containers:XenosEntryModule xmlns:mx="http://www.adobe.com/2006/mxml" 
							xmlns:containers="com.nri.rui.core.containers.*" 
							xmlns:flexlib="http://code.google.com/p/flexlib/" 
							xmlns:cntrls="com.nri.rui.core.controls.*" 
							xmlns:xenospopup="com.nri.rui.ref.popupImpl.*" 
							xmlns:frmtrs="com.nri.rui.core.formatters.*" 
							xmlns:valdtrs="com.nri.rui.core.validators.*" 
							layout="vertical" 
							percentWidth="100" 
							percentHeight ="100" 
							horizontalScrollPolicy="off" 
							verticalScrollPolicy="off">
    <mx:Script>
    	<![CDATA[
    		import mx.resources.ResourceBundle;
    		import com.nri.rui.core.utils.XenosPopupUtils;
    		
    		private function displayVersionNo(item:Object,column : DataGridColumn):String{
            	return item.refNo+"-"+item.verNo;
            	
            }
          /**
		   * This method is used for loading Account Details popup module
		   * 
		   */  
		   public function showAccountDetail(accPk:String):void{
		   	var parentApp :UIComponent = UIComponent(this.parentApplication);
					XenosPopupUtils.showAccountSummary(accPk,parentApp);
					
		   }
    		
    	]]>
    </mx:Script>
	
	<mx:Script source="actionscripts/SlrTradeEntryModule.as"/>
	 
    <containers:states>
    	<mx:State name="result">
            <mx:SetProperty target="{qry}" name="width" value="0%"/>
            <mx:SetProperty target="{rslt}" name="width" value="100%"/>
        </mx:State>
        <mx:State name="entryState">
            <mx:SetProperty target="{qry}" name="width" value="100%"/>
            <mx:SetProperty target="{rslt}" name="width" value="0%"/>
        </mx:State>
    </containers:states>
    <!-- Define Transition array with one Transition object.-->
    <containers:transitions>
        <!-- A transition for changing from any state to any state. -->
        <mx:Transition id="qryTransition" fromState="*" toState="*">
            <!-- Define a Parallel effect as the top-level effect.-->
            <mx:Parallel id="t1" targets="{[qry,rslt]}">
                <!-- Define a Move and Resize effect.-->
                <mx:Move  duration="400"/>
                <!--<mx:Resize duration="400"/>-->
            </mx:Parallel>
        </mx:Transition>
    </containers:transitions>
	
	<cntrls:XenosHTTPService id="addSecurityRequest" 
         					url="slr/slrContractEntryDispatch.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadSecurity(event)"  
         					method="POST"
         					showBusyCursor="true"/>

	<cntrls:XenosHTTPService id="editSecurityRequest" 
         					url="slr/slrContractEntryDispatch.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadEditedSecurity(event)" 
         					showBusyCursor="true"/>

	<cntrls:XenosHTTPService id="deleteSecurityRequest" 
         					url="slr/slrContractEntryDispatch.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadDeletedSecurity(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="calculationRequest" 
         					url="slr/slrContractEntryDispatch.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadCalculatedFields(event)"  
         					method="POST"
         					showBusyCursor="true"/>


	<cntrls:XenosHTTPService id="resetSecuritySsiRequest" 
         					url="slr/slrContractEntryDispatch.action?"  
         					method="POST"
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadResetSecuritySsi(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="resetCashSsiRequest" 
         					url="slr/slrContractEntryDispatch.action?"  
         					method="POST"
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadResetCashSsi(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="cancelMtmCloseOut" 
         					url="slr/SlrCancelDispatch.action?"  
         					method="POST"
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadCancelMtmResult(event)"
         					resultFormat="xml"
         					showBusyCursor="true"/>
         					
     <cntrls:XenosHTTPService id="cancelBack" 
         					url="slr/SlrCancelDispatch.action?"  
         					method="POST"
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('slr.label.erroroccured') + event.fault.faultString)"
         					result="loadCancelBackResult(event)"
         					resultFormat="xml"
         					showBusyCursor="true"/>					

	<frmtrs:XenosNumberFormatter id="numberFormatter" useThousandsSeparator="true" />
	<valdtrs:XenosNumberValidator id="hairCutVal" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidhaircut')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsbeforedecimal')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidhaircut')}" 
							    	separationError="" 
							    	source="{hairCut}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidhaircut')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidhaircut')}" 
        							precision="5" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.5digitsafterdecimal')}" />
        							
    <valdtrs:XenosNumberValidator id="divRateVal" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invaliddividentrate')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsbeforedecimal')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invaliddividentrate')}" 
							    	separationError="" 
							    	source="{dividendRate}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invaliddividentrate')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invaliddividentrate')}" 
        							precision="5" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.5digitsafterdecimal')}" />
        							
    <valdtrs:XenosNumberValidator id="IntRateVal" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestrate')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsbeforedecimal')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestrate')}" 
							    	separationError="" 
							    	source="{interestRate}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="99999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestrate')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestrate')}" 
        							precision="7" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.7digitsafterdecimal')}" />
	
	<valdtrs:XenosNumberValidator id="IntAmtVal" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestamount')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsbeforedecimal')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestamount')}" 
							    	separationError="" 
							    	source="{interestAmount}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999999999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestamount')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidinterestamount')}" 
        							precision="3" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsafterdecimal')}" />
        							
     <valdtrs:XenosNumberValidator id="commVal" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidcommission')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsbeforedecimal')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidcommission')}" 
							    	separationError="" 
							    	source="{commission}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidcommission')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidcommission')}" 
        							precision="5" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.5digitsafterdecimal')}" />
    
    
    <valdtrs:XenosNumberValidator id="subLimitVal" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidsublimit')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.3digitsbeforedecimal')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidsublimit')}" 
							    	separationError="" 
							    	source="{subLimit}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidsublimit')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.invalidsublimit')}" 
        							precision="5" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('slr.validation.error.5digitsafterdecimal')}" />						
        							
	
    <mx:Canvas width="100%" height="100%" horizontalScrollPolicy="auto"  verticalScrollPolicy="auto">
	<mx:VBox width="100%" height="100%">
	<mx:ViewStack id="vstack" width="100%" height="100%" creationComplete="loadAll()" creationPolicy="all">
		
		<mx:Canvas id="qry" width="100%" height="100%" horizontalScrollPolicy="auto" verticalScrollPolicy="auto">
		<mx:VBox label="{this.parentApplication.xResourceManager.getKeyValue('slr.label.entrycriteria')}" width="100%" height="100%">
			<mx:Label id="initLabel" text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.tradeentry')}" fontWeight="bold"/>
			<cntrls:XenosErrors id="errPage" width="100%" color="#FF0000" backgroundAlpha="0" borderStyle="none"/>
			<mx:HRule strokeColor="#94acb7" strokeWidth="2" width="100%" />
			<mx:Grid id="entryGrid" width="100%" borderThickness="0">
			
			        <mx:GridRow width="100%">
					<mx:GridItem width="20%" borderThickness="0">
						<mx:Label id="contractNoLbl" text="{this.parentApplication.xResourceManager.getKeyValue('slr.targetcontract.popup.label.contractno')}" />
					</mx:GridItem>
					<mx:GridItem width="30%" borderThickness="0">
						<mx:Label id="contractNo" selectable="true"  text="" fontWeight="bold"/>	    
					</mx:GridItem>
					<mx:GridItem width="20%" borderThickness="0">
						<mx:Label id="openEndedLbl" text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.openended')}" styleName="ReqdLabel"/>
					</mx:GridItem>
					<mx:GridItem width="30%" borderThickness="0">
					<mx:Label id="openEndedVal" selectable="true"  text=""/>	 
					</mx:GridItem>
				</mx:GridRow>
                    <mx:GridRow width="100%">
                        <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.tradetype')}" styleName="ReqdLabel"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <mx:ComboBox id="tradeType" prompt="Select" change="changeSubTradeType();changeSublimit();resetSsiInfo();changeExpirationDate();" />
                        </mx:GridItem> 
                        <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.subtradetype')}"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                           <mx:ComboBox id="subTradeType" prompt="Select" labelField="label" change="resetSsiInfo();disableTradeDate();changeSettlementCCY();"/>
                        </mx:GridItem>                         
                    </mx:GridRow>
                    <mx:GridRow width="100%">
	                       <mx:GridItem width="20%" borderThickness="0">
	                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.accountno')}" styleName="ReqdLabel"/>
	                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<xenospopup:AccountPopUpHbox id="brokerAccPopUp" retContextItem="{returnContextItem}" recContextItem="{populateActContext()}"/>
                        </mx:GridItem> 
	                        <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.rr')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <xenospopup:SalesPopUpHbox id="rr"/>                           
                        </mx:GridItem>                      
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                         <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.inventoryaccount')}" styleName="ReqdLabel"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <xenospopup:AccountPopUpHbox id="fundAccountPopup" retContextItem="{returnContextItem}" recContextItem="{populateInvActContext()}"/>
                        </mx:GridItem> 
                         <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.executingrr')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <xenospopup:SalesPopUpHbox id="executingRr"/>                           
                        </mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                       <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.traderid')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <xenospopup:SalesPopUpHbox id="traderId"/>                           
                        </mx:GridItem>
                         <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.haircut')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <mx:TextInput id="hairCut" editable="true" focusOut="hairCutVal.handleNumericField(numberFormatter);"/>                           
                        </mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                       <mx:GridItem width="20%" borderThickness="0">
							<mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.brokercode')}"/>
						</mx:GridItem>
						 <mx:GridItem width="30%" borderThickness="0">
							<xenospopup:FinInstitutePopUpHbox id="brokerCode"/>
						</mx:GridItem>   
                         <mx:GridItem width="20%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.extrefno')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                            <mx:TextInput id="extRefNo" editable="true" restrict="{Globals.INPUT_PATTERN}"/>                           
                        </mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%" id="actBalTypeGrd">
                        <mx:GridItem width="20%" borderThickness="0">
							<mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.accountbalancetype')}" styleName="ReqdLabel" />
						</mx:GridItem>
						<mx:GridItem width="30%" borderThickness="0">
							<mx:ComboBox id="accBalType" prompt="Select" labelField="label"/>
						</mx:GridItem>  
                        <mx:GridItem width="20%" borderThickness="0"/>
						<mx:GridItem width="30%" borderThickness="0"/>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                        <mx:GridItem width="20%" borderThickness="0">
							<mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.tradecurrency')}"/>
						</mx:GridItem>
						<mx:GridItem width="30%" borderThickness="0">
							<cntrls:CurrencyHBox id="tradeCurrency"/>
						</mx:GridItem>  
                         <mx:GridItem width="20%" borderThickness="0">
							<mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.settlementcurrency')}"/>
						</mx:GridItem>
						<mx:GridItem width="30%" borderThickness="0">
							<cntrls:CurrencyHBox id="settlementCurrency"/>
						</mx:GridItem>  
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                        <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.tradedate')}" styleName="ReqdLabel" />
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:DateField id="tradeDate"  formatString="YYYYMMDD" editable="true" width="100"/>
                        	<mx:TextInput id="tradeTime" editable="true" focusOut="tradeTime.text = validateTime(tradeTime.text)" width="90"/>
                        </mx:GridItem>   
                        <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.startdate')}" styleName="ReqdLabel"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:DateField id="startDate"  formatString="YYYYMMDD" editable="true" width="100"/>
                        </mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                       <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.expirationdate')}"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:DateField id="expirationDate"  formatString="YYYYMMDD" editable="true" width="100"/>
                        </mx:GridItem>
                        <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.enddate')}"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:DateField id="endDate" formatString="YYYYMMDD" editable="true" width="100"/>
                        </mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                       <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.dividendrate')}"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
							<mx:TextInput id="dividendRate" editable="true" focusOut="divRateVal.handleNumericField(numberFormatter);"/>
						</mx:GridItem>
                        <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.accrualdays')}"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:Image source="@Embed('../../assets/img/slr/icn_calculator.png')" toolTip="{this.parentApplication.xResourceManager.getKeyValue('slr.label.calcaccrdays')}" buttonMode="true" click="calculateAccruedDays();"/>
							<mx:TextInput id="accrualDays" editable="false"/>
						</mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                       <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.interestrate')}" styleName="ReqdLabel"/>
	                   </mx:GridItem>
                       <mx:GridItem width="30%" borderThickness="0">
							<mx:TextInput id="interestRate" editable="true" focusOut="IntRateVal.handleNumericField(numberFormatter);"/>
						</mx:GridItem>
                        <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.interestamount')}"/>
	                    </mx:GridItem>
                        <mx:GridItem width="30%" borderThickness="0">
                        	<mx:Image source="@Embed('../../assets/img/slr/icn_calculator.png')" toolTip="{this.parentApplication.xResourceManager.getKeyValue('slr.label.calcinterestamt')}" buttonMode="true" click="calculateInterestAmount();"/>
                        	<mx:TextInput id="interestAmount" editable="true" focusOut="IntAmtVal.handleNumericField(numberFormatter);"/>
						</mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                       <mx:GridItem width="20%" borderThickness="0">
	                        <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('slr.contract.label.commission')}"/>
	                    </mx:GridItem>
