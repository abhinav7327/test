            }
          /**
		   * This method is used for loading Account Details popup module
		   * 
		   */  
		   public function showAccountDetail(accPk:String):void{
			   	var parentApp :UIComponent = UIComponent(this.parentApplication);
				XenosPopupUtils.showAccountSummary(accPk,parentApp);
		   }
		   
		   public function formatPrice(event:FocusEvent):void{
		        var targetObj:TextInput = TextInput(event.currentTarget);
		        priceVal.source = targetObj;
		        priceVal.handleNumericField(numberFormatter);
		   }
		   
		   public function formatPricipalAmt(event:FocusEvent):void{
		        var targetObj:TextInput = TextInput(event.currentTarget);
		        pricipalAmtVal.source = targetObj;
		        pricipalAmtVal.handleNumericField(numberFormatter);
		   }
		   
		   public function formatInterestAmt(event:FocusEvent):void{
		        var targetObj:TextInput = TextInput(event.currentTarget);
		        interestAmtVal.source = targetObj;
		        interestAmtVal.handleNumericField(numberFormatter);
		   }
		   
		   public function formatNetAmt(event:FocusEvent):void{
		        var targetObj:TextInput = TextInput(event.currentTarget);
		        netAmtVal.source = targetObj;
		        netAmtVal.handleNumericField(numberFormatter);
		   }
		   
    	]]>
