<?xml version="1.0" encoding="utf-8"?>
<containers:XenosEntryModule xmlns:mx="http://www.adobe.com/2006/mxml" 
							xmlns:containers="com.nri.rui.core.containers.*" 
							xmlns:flexlib="http://code.google.com/p/flexlib/" 
							xmlns:cntrls="com.nri.rui.core.controls.*" 
							xmlns:xenospopup="com.nri.rui.ref.popupImpl.*" 
							xmlns:frmtrs="com.nri.rui.core.formatters.*" 
							xmlns:valdtrs="com.nri.rui.core.validators.*"
							layout="vertical" 
							percentWidth="100" 
							percentHeight ="100" 
							horizontalScrollPolicy="off" 
							verticalScrollPolicy="off">
    <mx:Script>
    	<![CDATA[
    		import com.nri.rui.core.Globals;
    		import mx.resources.ResourceBundle;
    	]]>
    </mx:Script>
	
    <mx:Script source="actionscripts/TradeEntryModule.as"/>
	 
    <containers:states>
    	<mx:State name="result">
            <mx:SetProperty target="{qry}" name="width" value="0%"/>
            <mx:SetProperty target="{rslt}" name="width" value="100%"/>
        </mx:State>
        <mx:State name="entryState">
            <mx:SetProperty target="{qry}" name="width" value="100%"/>
            <mx:SetProperty target="{rslt}" name="width" value="0%"/>
        </mx:State>
    </containers:states>
    <!-- Define Transition array with one Transition object.-->
    <containers:transitions>
        <!-- A transition for changing from any state to any state. -->
        <mx:Transition id="qryTransition" fromState="*" toState="*">
            <!-- Define a Parallel effect as the top-level effect.-->
            <mx:Parallel id="t1" targets="{[qry,rslt]}">
                <!-- Define a Move and Resize effect.-->
                <mx:Move  duration="400"/>
                <!--<mx:Resize duration="400"/>-->
            </mx:Parallel>
        </mx:Transition>
    </containers:transitions>
    <cntrls:XenosHTTPService id="addlTradeEntryRequest" 
         					url="trd/tradeEntry.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadAdditionalTradeEntryScreen(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="resetRequest" 
         					url="trd/tradeEntry.action?method=view" 
         					method="POST" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadResetTradeEntryScreen(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="resetSecuritySsiRequest" 
         					url="trd/tradeEntry.action?"  
         					method="POST"
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadResetSecuritySsi(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="resetCashSsiRequest" 
         					url="trd/tradeEntry.action?"  
         					method="POST"
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadResetCashSsi(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="addTaxInfoRequest" 
         					url="trd/tradeEntry.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="addTaxInfoResultHandler(event)" 
         					showBusyCursor="true"/>
        					
    <cntrls:XenosHTTPService id="editTaxInfoRequest" 
         					url="trd/tradeEntry.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="editTaxInfoResultHandler(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="deleteTaxInfoRequest" 
         					url="trd/tradeEntry.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="deleteTaxInfoResultHandler(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="backToFirstEntryScreenRequest" 
         					url="trd/tradeEntry.action?method=backtoInitialScreen" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadFirstEntryScreenFromSecondHandler(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="backToFirstEntryScreenRequestForAmend" 
         					url="trd/tradeEntryAmend.action?method=backtoInitialScreen" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadFirstEntryScreenFromSecondHandler(event)" 
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="calcNetAmtRequest" 
         					url="trd/tradeEntry.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadCalculatedNetAmount(event)"  
         					method="POST"
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="cancelConfRequest" 
         					url="trd/tradeCancel.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadCancelConfirmation(event)"  
         					method="POST"
         					showBusyCursor="true"/>
         					
    <cntrls:XenosHTTPService id="cancelSystemConfForRequest" 
         					url="trd/tradeCancel.action?" 
         					fault="XenosAlert.error(this.parentApplication.xResourceManager.getKeyValue('inf.label.error.occurred') + event.fault.faultString)"
         					result="loadSystemConfirmationForCancel(event)"  
         					method="POST"
         					showBusyCursor="true"/>
         					
    <frmtrs:XenosNumberFormatter id="numberFormatter" useThousandsSeparator="true" />
	<valdtrs:XenosNumberValidator id="numValQty" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.quantity.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.quqntity.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.quantity.error')}" 
							    	separationError="" 
							    	source="{quantity}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999999999999999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.quantity.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.quantity.error')}" 
        							precision="3" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.quqntity.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValInputPrice" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.inputprice.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.inputprice.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.inputprice.error')}" 
							    	separationError="" 
							    	source="{inputPrice}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999999999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.inputprice.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.inputprice.error')}" 
        							precision="9" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.inputprice.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValIndexRatio" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.indexratio.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.indexratio.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.zero.indexratio.error')}" 
							    	separationError="" 
							    	source="{indexRatio}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="{this.parentApplication.xResourceManager.getKeyValue('trd.zero.indexratio.error')}"
        							maxValue="99999999.9999999999"
        							minValue="0.0000000001" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.indexratio.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.indexratio.error')}" 
        							precision="10" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.indexratio.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValExchngRate" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.exchangerate.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.exchangerate.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.exchangerate.error')}" 
							    	separationError="" 
							    	source="{exchangeRate}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="9999999999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.exchangerate.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.exchangerate.error')}" 
        							precision="10" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.exchangerate.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValIssuePricingExchngRate" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.issuepricingexchangerate.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.issuepricingexchangerate.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.issuepricingexchangerate.error')}" 
							    	separationError="" 
							    	source="{issuePricingExchangeRate}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="9999999999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.issuepricingexchangerate.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.issuepricingexchangerate.error')}" 
        							precision="10" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.issuepricingexchangerate.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValAccrIntAmt" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrinterestamnt.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.acrinterestamnt.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrinterestamnt.error')}" 
							    	separationError="" 
							    	source="{accrInterestAmt}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999999999999999.999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrinterestamnt.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrinterestamnt.error')}" 
        							precision="3" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.acrinterestamnt.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValAccrDays" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrdays.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.acrdays.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrdays.error')}" 
							    	separationError="" 
							    	source="{accrDays}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="99999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrdays.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.acrdays.error')}" 
        							precision="0" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.acrdays.precision.error')}" />
    
    <valdtrs:XenosNumberValidator id="numValTaxRate" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxrate.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.taxrate.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxrate.error')}" 
							    	separationError="" 
							    	source="{taxRateTxt}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxrate.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxrate.error')}" 
        							precision="5" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.taxrate.precision.error')}" />
    <!--Negative Tax Fee Amount is allowed according to the discussion on XGA-1122-->    							
    <valdtrs:XenosNumberValidator id="numValTaxAmt" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxamnt.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.taxamnt.digit.error')}"
							    	separationError="" 
							    	source="{taxAmountTxt}" 
							    	property="text"
        							domain="real"  
        							allowNegative="true"
        							lowerThanMinError="" 
        							maxValue="9999999999" 
        							minValue="-9999999999" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxamnt.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.taxamnt.error')}" 
        							precision="3" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.taxamnt.precision.error')}" />
        							
    <valdtrs:XenosNumberValidator id="numValPrincipalAmt" 
									requiredFieldError=""
    								invalidCharError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.input.principalamount.error')}" 
							    	exceedsMaxError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.input.principalamount.digit.error')}" 
    								negativeError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.input.principalamount.precision.error')}" 
							    	separationError="" 
							    	source="{principalAmt}" 
							    	property="text" 
        							allowNegative="false" 
        							domain="real" 
        							lowerThanMinError="" 
        							maxValue="999999999999999" 
        							minValue="" 
        							invalidFormatCharsError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.originalfacevalue.error')}" 
        							decimalPointCountError="{this.parentApplication.xResourceManager.getKeyValue('trd.invalid.originalfacevalue.error')}" 
        							precision="3" 
        							precisionError="{this.parentApplication.xResourceManager.getKeyValue('trd.allowed.originalfacevalue.precision.error')}" />	    							
    <mx:Canvas width="100%" height="100%" horizontalScrollPolicy="auto"  verticalScrollPolicy="auto">
	<mx:VBox width="100%" height="100%">
	<mx:ViewStack id="vstack" width="100%" height="100%" creationComplete="loadAll()" creationPolicy="all">
		
		<mx:Canvas id="qry" width="100%" height="100%" horizontalScrollPolicy="auto" verticalScrollPolicy="auto" >
		<mx:VBox label="Entry Criteria" width="100%" height="100%">
			<mx:Label id="initLabel" text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradequeryaction.label.entry')}" fontWeight="bold"/>
			<cntrls:XenosErrors id="errPage" width="100%" color="#FF0000" backgroundAlpha="0" borderStyle="none"/>
			<mx:HRule strokeColor="#94acb7" strokeWidth="2" width="100%" />
			<mx:VBox id="amendRefNoBox" label="Trade Amend" width="100%" visible="false" includeInLayout="false">
				<mx:Grid width="100%" borderThickness="1" horizontalGap="0" verticalGap="0">
                    <mx:GridRow width="100%">
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradequeryaction.label.referenceno')}" fontWeight="bold"/>
                        </mx:GridItem>
                        <mx:GridItem width="90%" borderThickness="0">
                            <mx:Label id="amendRefNo" text="" fontWeight="bold"/>
                        </mx:GridItem>
                    </mx:GridRow>
                </mx:Grid>
			</mx:VBox>
			<mx:Grid width="100%" borderThickness="0">
                    <mx:GridRow width="100%">
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.tradetype')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="19%" borderThickness="0">
                            <mx:ComboBox id="tradeType" prompt="Select" labelField="label" change="checkTradeType();"/>
                            <mx:TextInput id="tradeTypeTxt" editable="false" width="100" visible="false" includeInLayout="false" enabled="false" disabledColor="0x000000"/>
                        </mx:GridItem> 
                        <mx:GridItem width="15%" borderThickness="0">
	                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.tradedate')}" styleName="ReqdLabel" />
	                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:DateField id="tradeDate"  formatString="YYYYMMDD" editable="true"/>
                        	<mx:TextInput id="tradeDateTxt" editable="false" width="100" visible="false" includeInLayout="false" enabled="false" disabledColor="0x000000"/>
                        	<mx:TextInput id="tradeTime" editable="true" focusOut="tradeTime.text = validateTime(tradeTime.text)" width="90"/> 
                        </mx:GridItem> 
                        <mx:GridItem width="15%" borderThickness="0">
	                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.valuedate')}"/>
	                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:DateField id="valueDate"  formatString="YYYYMMDD" editable="true" width="100"/>
                        </mx:GridItem>                        
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.buysellflag')}" styleName="ReqdLabel"/>
                        </mx:GridItem>
                        <mx:GridItem width="19%" borderThickness="0">
                            <mx:ComboBox id="buySellFlag" prompt="Select" labelField="label" change="checkBuySell();"/>
                            <mx:TextInput id="buySellFlagTxt" editable="false" width="100" visible="false" includeInLayout="false" enabled="false" disabledColor="0x000000"/>
                        </mx:GridItem>  
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.matchingsupressflag')}"/>
                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:ComboBox id="matchingSuppressFlag" prompt="Select" labelField="label"/>
                        </mx:GridItem>
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text=""/>
                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:Label text=""/> 
                        </mx:GridItem>
                        <!--  //XGA-105
                        <mx:GridItem width="15%" borderThickness="0">
	                            <mx:Label id="shortSellFlagLbl" text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.shortsellingflag')}"/>
	                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:ComboBox id="shortSellFlag" prompt="Select" labelField="label"/> 
                        </mx:GridItem> 
                        <mx:GridItem width="15%" borderThickness="0">
	                            <mx:Label id="shortSellExemptFlagLbl" text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.shortsellexemptflag')}"/>
	                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:ComboBox id="shortSellExemptFlag" prompt="Select" labelField="label"/>
                        </mx:GridItem>     
                        -->                   
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.inventoryaccountno')}" styleName="ReqdLabel"/>
                        </mx:GridItem>
                        <mx:GridItem width="19%" borderThickness="0">
                            <xenospopup:AccountPopUpHbox id="fundAccountPopup" retContextItem="{returnContextItem}" recContextItem="{populateInvActContext()}"/>
                            <mx:TextInput id="fundAccNoTxt" editable="false" width="100" visible="false" includeInLayout="false" enabled="false" disabledColor="0x000000"/>
                            <!--<mx:TextInput id="fundAccName" width="90" editable="false" toolTip="{fundAccName.text}" enabled="false" disabledColor="0x000000"/>-->
                            <cntrls:XenosLabel id="fundAccName" toolTip="{fundAccName.text}"/>
                        </mx:GridItem>    
                        <mx:GridItem width="15%" borderThickness="0">
                        	<mx:Label id="plSuppressFlagLabel" text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.plsuppressflag')}" includeInLayout="false" visible="false"/>
                        </mx:GridItem> 
                         <mx:GridItem width="19%" borderThickness="0">
                        	<mx:ComboBox id="plSuppressFlag" labelField="label" includeInLayout="false" visible="false" change="checkPlSuppressFlag()"/>
                        </mx:GridItem>  
                         <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text=""/>
                        </mx:GridItem>
                        <mx:GridItem width="18%" borderThickness="0">
                        	<mx:Label text=""/> 
                        </mx:GridItem>               
                    </mx:GridRow>
                    <mx:GridRow width="100%"> 
                        <mx:GridItem width="15%" borderThickness="0">
	                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradeentryaction.label.brokeraccountno')}" styleName="ReqdLabel"/>
	                        </mx:GridItem>
                        <mx:GridItem width="85%" borderThickness="0" colSpan="5">
                        	<xenospopup:AccountPopUpHbox id="brokerAccPopUp" retContextItem="{returnContextItem}" recContextItem="{populateActContext()}"/>
                        	<mx:TextInput id="brokerAccNoTxt" editable="false" width="100" visible="false" includeInLayout="false" enabled="false" disabledColor="0x000000"/>
                        	<!--<mx:TextInput id="brokerAccName" width="90" editable="false" toolTip="{brokerAccName.text}" enabled="false" disabledColor="0x000000"/>-->
                        	<cntrls:XenosLabel id="brokerAccName" toolTip="{brokerAccName.text}" width="500"/>  
                        </mx:GridItem>
                    </mx:GridRow>
                    <mx:GridRow width="100%">
                        <mx:GridItem width="15%" borderThickness="0">
                            <mx:Label text="{this.parentApplication.xResourceManager.getKeyValue('trd.tradequeryaction.label.securitycode')}" styleName="ReqdLabel"/>
