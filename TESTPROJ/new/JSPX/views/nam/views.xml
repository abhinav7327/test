<tiles-definitions>

	<!-- Fund Config Query Criteria page -->
	<definition name="fundConfigQueryCriteria" extends="queryCriteria">
		<put-attribute name="content">
			<definition extends="criteria_and_order">
				<put-attribute name="criteria"
					value="/WEB-INF/views/nam/fundConfigQueryCriteria.jspx" />
			</definition>
		</put-attribute>
	</definition>

	<!-- Fund Config Query Result page -->
	<definition name="fundConfigQueryResult" extends="result">
		<put-attribute name="content">
			<definition extends="queryResult">
				<put-attribute name="resultGrid"
					value="/WEB-INF/views/nam/fundConfigQueryResult.jspx" />
			</definition>
		</put-attribute>
	</definition>

	<!-- Fund Config Amend View Page -->
	<definition name="fundConfigAmendForm" extends="wizard-page-template">
		<put-attribute name="wizard-page" value="/WEB-INF/views/nam/fundConfigAmendForm.jspx"/>
	</definition>
	
	 <!-- Fund Config Detail page -->
	 <definition name="fundConfigAmendDetail" extends="wizard-page-template">
	    <put-attribute name="wizard-page" value="/WEB-INF/views/nam/fundConfigAmendDetail.jspx"/>
	 </definition>

   <!-- TDBalanceQueryCriteria page -->
    <definition name="tdBalanceQueryCriteria" extends="queryCriteria">
    <put-attribute name="content">
      <definition extends="criteria_and_order">
        <put-attribute name="criteria" value="/WEB-INF/views/nam/tdBalanceQueryCriteria.jspx"/>
      </definition>
    </put-attribute>
  </definition>

  <!-- TDBalanceQueryResult page -->
    <definition name="tdBalanceQueryResult" extends="result">
    <put-attribute name="content">
      <definition extends="queryResult">
        <put-attribute name="resultGrid" value="/WEB-INF/views/nam/tdBalanceQueryResult.jspx"/>
      </definition>
    </put-attribute>
  </definition>
  
  <!-- TDBalance Detail View -->
    <definition name="tdBalanceDetailView" extends="detailview-wrapper">
	<put-attribute name="content">
	  <definition extends="detailView">
	    <put-attribute name="detailView" value="/WEB-INF/views/nam/tdBalanceDetailView.jspx"/>
	  </definition>
    </put-attribute>
    </definition>
  
</tiles-definitions>
