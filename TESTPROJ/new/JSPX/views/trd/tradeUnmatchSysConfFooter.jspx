<div version="2.0"
     xmlns:jsp="http://java.sun.com/JSP/Page"
     xmlns:c="http://java.sun.com/jsp/jstl/core"
     xmlns:form="http://www.springframework.org/tags/form"
     xmlns:fn="http://java.sun.com/jsp/jstl/functions"
     xmlns:spring="http://www.springframework.org/tags"
     xmlns:util="urn:jsptagdir:/WEB-INF/tags/util">

    <jsp:directive.page contentType="text/html;charset=UTF-8"/>
    <jsp:output omit-xml-declaration="yes"/>
    
    
    <div id="formActionArea">
			<div class="left">
				<spring:message code="trd.allocation.cxl.query.btn.ok" htmlEscape="false" var="ok_label"/>
				
				<div class="resultBtnBlock">
					<div class="btnWrapStyle submitBtn umnatchOkSubmitBtn"><input type="submit" value="${ok_label}" class="inputBtnStyle"/></div>
				</div>
				<div class="clear"><spring:message text="" htmlEscape="false"/></div>
			</div>
			
			<div class="clear"><spring:message text="" htmlEscape="false"/></div>
		</div>
		
			<jsp:element name="script">
				<jsp:attribute name="type">text/javascript</jsp:attribute>
				<jsp:attribute name="src"><c:url value="/scripts/trd/xenos/trdCnfAllocCxl.js"/></jsp:attribute>
				<jsp:body></jsp:body>
			</jsp:element>
		
		<jsp:element name="script">
		<jsp:attribute name="type">text/javascript</jsp:attribute>
		<jsp:body>
		
			
		xenos$onReady$Array.push(function() {
        var scripts = [
          {path: xenos.context.path + '/scripts/trd/xenos/trdCnfAllocCxl.js'}
          
        ];  	
        xenos.loadScript(scripts, {
          ordered: true,
          success : function() {
              var $context = $('#formContainer');
			  $('.umnatchOkSubmitBtn', $context).unbind('click',xenos.ns.trdMatch.okhandler);								
              $('.umnatchOkSubmitBtn', $context).bind('click',xenos.ns.trdMatch.okhandler);
				
				
				$allocCxl$contaxt = $('#commandForm');
					$($allocCxl$contaxt).unbind('keydown').bind('keydown', function(e){
					  var key = e.which || e.keyCode;
					  if(key == 13){
						$('.umnatchOkSubmitBtn', $allocCxl$contaxt).trigger('click');
					  }
					});
          }
        });
		});
      </jsp:body>
      </jsp:element>		
    
</div>
    